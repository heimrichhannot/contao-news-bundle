{
    "name": "heimrichhannot/contao-news-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 news bundle",
    "license": "LGPL-3.0+",
    "require": {
        "php": "~5.4 || ~7.0",
        "contao/core-bundle": "^3.5.1 || ~4.1",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0",
        "codefog/contao-news_categories": "^2.8",
        "codefog/tags-bundle": "~2.0",
        "heimrichhannot/contao-tagsinput": "^2.1",
        "google/apiclient": "^2.0",
        "codefog/contao-haste": "^4.1"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^4.5",
        "satooshi/php-coveralls": "^1.0",
        "symfony/phpunit-bridge": "^2.8|^3.0"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\NewsBundle\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HeimrichHannot\\NewsBundle\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\NewsBundle\\ContaoManager\\Plugin"
    }
}
